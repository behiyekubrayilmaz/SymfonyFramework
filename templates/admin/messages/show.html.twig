{% extends 'adminbase.html.twig' %}

{% block title %}House index{% endblock %}

{% block body %}

    <div class="span9">
        <div class="content">
            <div class="module">
                <div class="module-head">
                    <h3>
                        Messages
                    </h3>
                </div>
                <div class="module-body">

                    <table class="table table-hover">
                        <tbody>
                        <tr>
                            <th>Id</th>
                            <td>{{ message.id }}</td>
                        </tr>
                        <tr>
                            <th>Name</th>
                            <td>{{ message.name }}</td>
                        </tr>
                        <tr>
                            <th>Email</th>
                            <td>{{ message.email }}</td>
                        </tr>
                        <tr>
                            <th>Subject</th>
                            <td>{{ message.subject }}</td>
                        </tr>
                        <tr>
                            <th>Message</th>
                            <td>{{ message.message }}</td>
                        </tr>
                        <tr>
                            <th>Status</th>
                            <td>{{ message.status }}</td>
                        </tr>
                        <tr>
                            <th>Ip</th>
                            <td>{{ message.ip }}</td>
                        </tr>
                        <tr>
                            <th>Note</th>
                            <td>{{ message.note }}</td>
                        </tr>
                        <tr>
                            <th>Created_at</th>
                            <td>{{ message.createdAt ? message.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                        </tr>

                        <br>
                        <tr>

                            <td><a href="{{ path('admin_messages_index') }}" class="btn btn-info " >
                                    <i class="icon-arrow-left"></i>Back to list</a></td>
                        </tr>
                        </tbody>
                    </table>




                </div>

            </div>
        </div>
    </div><!--/.content-->
    </div><!--/.span9-->
    </div>
    </div>
    <!--/.container-->
    </div>
    <!--/.wrapper-->
{% endblock %}


